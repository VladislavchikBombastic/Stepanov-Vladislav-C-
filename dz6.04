#include <iostream>

using namespace std;
// 1 
void chislo(int a) {
	if (a < 999999 && a > 99999) {
		int a1, a2, a3;
		int b1, b2, b3;
		b1 = (a / 100) % 10;
		b2 = (a / 10) % 10;
		b3 = a % 10;
		a1 = a / 100000;
		a2 = (a / 10000) % 10;
		a3 = (a / 1000) % 10;
		if (a1 + a2 + a3 == b1 + b2 + b3) {
			cout << "happy";
		}
		else {
			cout << "unhappy";
		}
	}
}
int main() {
	int a;
	cin >> a;
	chislo(a);
}
