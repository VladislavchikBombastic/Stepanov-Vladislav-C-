#include <iostream>

using namespace std;



// 1
int result(int a, int b) {
	int res = a;
	for (int i = 1; i < b; i++)
	{
		res = res *= a;
	}
	cout << res;
	return a, b;
}

int main() {
	setlocale(LC_ALL, "RUS");
	int num, degree;
	cout << "Число: ";
	cin >> num;
	cout << "Степень: ";
	cin >> degree;
	result(num, degree);
}
// 2
int diapazon(int a, int b) {
	int res = 0;
	for (int i = a + 1; i < b; i++) {
		res = res + i;
	}
	cout << res;
	return a, b;
}
int main() {
	int a, b;
	cin >> a >> b;
	diapazon(a, b);
}
